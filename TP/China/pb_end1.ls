 115:  IF R[221]=18,JMP LBL[1000] ;
 116:  ENDIF ;
 117:   ;
 118:  ! if one robot welds both sides ;
 119:  IF (R[290]<>0),JMP LBL[115] ;
 
 :;
  169:  !Signal to start R3 ;
 170:  R[75]=1    ;
 120:   ;
 121:  ! If not start of profile, jump t ;
 122:  IF ((R[95]=1) and (R[81]=2)) OR R[70]=55),JMP LBL[115] ;
