  21:  WAIT R[109]=0    ;
  22:  R[13]=6    ;
  23:  R[115]=1    ;
  24:  R[116]=1    ;
  25:  CALL WELD_PA    ;
  26:   ;
  27:  !-- AT RESUME JMP TO LABEL ;
  28:  IF ((R[151]=12)),JMP LBL[R[151]] ;
  29:  IF ((R[151]>100)),JMP LBL[R[151]] ;
  30:   ;
  31:   ;
  32:  !**************************** ;
  33:  !**** POSITION CALC & POINTS ;
  34:  !**************************** ;
  35:   ;
  36:  !* FUT 000 OR FUT 00-1 (PR[43-44) ;
  37:   ;
  38:  !***  POSITION CALC START ;
  39:  R[102]=(60+(R[95]*10)) ;
  40:   ;
  41:  !***  CONFIG CALC ;
  42:  R[103]=43    ;
  43:  IF (R[95]=(1)),R[103]=(45) ;
  44:   ;
  45:  CALL PRG_CALC1    ;
  46:   ;
  47:   ;
  48:  !Determind sensing point to use ;
  49:  R[101]=R[102]+1    ;
  50:  IF (R[254]=2),R[101]=R[102]+2 ;
  51:  IF (R[254]=3),R[101]=R[102]+2 ;
  52:  IF (R[254]=4),R[101]=R[102]+2 ;
  53:  IF (R[254]=7),R[101]=R[102]+2 ;
  54:   ;
  55:   ;
  56:  !Start Angle up point ;
  57:  !-- WELDING  POINT 5 ;
  58:  !Depending on OProfile flange ;
  59:  R[135]=(-20)    ;
  60:  IF (R[253]=2),R[135]=(-50) ;
  61:  R[141]=R[135]    ;
  62:  R[142]=(-1.4)    ;
  63:  CALL PRG_CALC1    ;
  64:  PR[5]=PR[R[101]]    ;
  65:  PR[5,1]=PR[5,1]+R[141]    ;
  66:  PR[5,2]=PR[5,2]+R[142]    ;
  67:  PR[5,3]=PR[5,3]+.2    ;
  68:   ;
  69:  !End Angle up point ;
  70:  ! also start R3 point ;
  71:  !-- WELDING  POINT 6 ;
  72:  R[135]=R[135]-100    ;
  73:  IF (R[135]<R[93]*(-1)),R[135]=R[93]*(-1) ;
  74:  R[141]=R[135]    ;
  75:  R[142]=(-1.4)    ;
  76:  CALL PRG_CALC1    ;
  77:  PR[6]=PR[21]+PR[R[101]]    ;
  78:  PR[6,1]=PR[6,1]+R[141]    ;
  79:  PR[6,2]=PR[6,2]+R[142]    ;
  80:  PR[6,3]=PR[6,3]+.2    ;
  81:   ;
  82:  !Move ahead point ;
  83:  !-- WELDING  POINT 7 ;
  84:  R[141]=((150)*(-1)) ;
  85:  ! Y-direction ;
  86:  IF (R[81]=2),R[141]=(200*(-1)) ;
  87:  R[142]=(-1.4)    ;
  88:  CALL PRG_CALC1    ;
  89:  PR[7]=PR[6]    ;
  90:  PR[7,1]=PR[7,1]+R[141]    ;
  91:  PR[7,2]=PR[7,2]+R[142]    ;
  92:  PR[7,3]=PR[7,3]+.2    ;
  93:  ! checking limit ;
  94:  IF (PR[7,1]<(-200)),PR[7,1]=(-200) ;
  95:   ;
  96:  !**************************** ;
  97:  ! Call sub programs for Starting ;
  98:  !**************************** ;
  99:  LBL[12] ;
 100:   ;
 101:  IF (R[254]=(1)) THEN ;
 102:  CALL PB_COR1    ;
 103:  IF R[151]=12,JMP LBL[1000] ;
 104:  ENDIF ;
 105:   ;
 106:  IF (R[254]=(2) OR R[254]=3 OR R[254]=4 OR R[254]=7) THEN ;
 107:  CALL PB_EDGE1    ;
