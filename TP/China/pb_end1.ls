 145:  PR[5,5]=PR[21,5]    ;
 146:  PR[5,6]=PR[21,6]    ;
 147:  CALL PRG_CALC1    ;
 148:  ENDIF ;
 149:   ;
 150:  Weave Sine[1] ;
 151:  Track TAST[R[21]] ;
 152:  //Weld Start E1[1,R[111],E0] ;
 153:   ;
 154:  R[134]=R[126]*1    ;
 155:L PR[5] R[126]cm/min CNT100    ;
 156:   ;
 157:   ;
 158:  !---------------------------- ;
