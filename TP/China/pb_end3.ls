 119:  PR[5,4]=PR[21,4]    ;
 120:  PR[5,5]=PR[21,5]    ;
 121:  PR[5,6]=PR[21,6]    ;
 122:  CALL PRG_CALC3    ;
 123:  ENDIF ;
 124:   ;
 125:  Weave Sine[3] ;
 126:  Track TAST[R[23]] ;
 127:  Weld Start E2[1,R[112],E0] ;
 128:   ;
 129:   ;
 130:L PR[5] R[127]cm/min CNT100    ;
 131:   ;
 132:  ! if one robot welds both sides ;
 133:  IF (R[290]<>0),JMP LBL[115] ;
 134:   ;
 135:  !For end_type=3 skip after this p ;
 136:  IF (((R[264]=3) OR (R[264]>=5)) AND (R[81]=1)),JMP LBL[999] ;
 137:   ;
 138:  !---------------------------- ;
 139:  !-- WELDING MOTION P6 ;
 140:  LBL[106] ;
 141:  R[153]=106    ;
 142:  Weave Sine[3] ;
 143:  Track TAST[R[23]] ;
 144:  Weld Start E2[1,R[112],E0] ;
 145:   ;
 146:   ;
 147:L PR[6] R[127]cm/min CNT100    ;
 148:  IF (R[290]=0),JMP LBL[999] ;
 149:   ;
 150:  ! If End of profile ;
 151:  ! Weld back to close point ;
 152:  LBL[115] ;
 153:  R[153]=115    ;
 154:  ! go in UF1 ;
 155:  UFRAME_NUM=1 ;
 156:   ;
 157:  PR[130]=LPOS    ;
 158:  PR[15,7]=PR[130,7]    ;
 159:   ;
 160:  R[104]=R[105]+15    ;
 161:  PR[15]=PR[R[104]]    ;
 162:   ;
 163:   ;
 164:  Weave Sine[3] ;
 165:  Track TAST[R[23]] ;
 166:  Weld Start E2[1,R[112],E0] ;
 167:   ;
 168:  CALL PRG_CALC3    ;
