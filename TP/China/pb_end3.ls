  23:  R[13]=6    ;
  24:  R[115]=1    ;
  25:  R[116]=3    ;
  26:  CALL WELD_PA    ;
  27:   ;
  28:  !-- AT RESUME JMP TO LABEL ;
  29:  IF ((R[153]>100)),JMP LBL[R[153]] ;
  30:   ;
  31:   ;
  32:   ;
  33:   ;
  34:  !**************************** ;
  35:  !**** POSITION CALC & POINTS ;
  36:  !**************************** ;
  37:   ;
  38:  !* FUT 000 OR FUT 00-1 (PR[43-44) ;
  39:   ;
  40:  !***  POSITION CALC START ;
  41:  R[105]=(60+(R[95]*10)) ;
  42:   ;
  43:  !***  CONFIG CALC ;
  44:  R[106]=43    ;
  45:  IF (R[95]=(1)),R[106]=(45) ;
  46:   ;
  47:  CALL PRG_CALC3    ;
  48:   ;
  49:   ;
  50:  !Determind sensing point to use ;
  51:  R[104]=R[105]+1    ;
  52:  IF (R[264]=2),R[104]=R[105]+2 ;
  53:  IF (R[264]=3),R[104]=R[105]+2 ;
  54:  IF (R[264]=4),R[104]=R[105]+2 ;
  55:  IF (R[264]=7),R[104]=R[105]+2 ;
  56:   ;
  57:   ;
  58:  !Start Angle up point ;
  59:  !-- WELDING  POINT 5 ;
  60:  !Depending on OProfile flange ;
  61:  R[138]=(-20)    ;
  62:  IF (R[263]=2),R[138]=(-50) ;
  63:  R[145]=R[138]    ;
  64:  R[146]=0    ;
  65:  CALL PRG_CALC3    ;
  66:  PR[5]=PR[R[104]]    ;
  67:  PR[5,1]=PR[5,1]+R[145]    ;
  68:  PR[5,2]=PR[5,2]+R[146]    ;
  69:   ;
  70:  !End Angle up point ;
  71:  !-- WELDING  POINT 6 ;
  72:  R[138]=R[138]-100    ;
  73:  R[145]=R[138]    ;
  74:  R[146]=0    ;
  75:  CALL PRG_CALC3    ;
  76:  PR[6]=PR[21]+PR[R[104]]    ;
  77:  PR[6,1]=PR[6,1]+R[145]    ;
  78:  PR[6,2]=PR[6,2]+R[146]    ;
  79:   ;
  80:  !**************************** ;
  81:  ! Call sub programs for Starting ;
  82:  !**************************** ;
  83:  IF (R[264]=(1)) THEN ;
  84:  CALL PB_COR3    ;
  85:  ENDIF ;
  86:  IF (R[264]=(2) OR R[264]=3 OR R[264]=4 OR R[264]=7) THEN ;
  87:  CALL PB_EDGE3    ;
  88:  ENDIF ;
  89:   ;
  90:  IF (R[264]=5 OR R[264]=6 OR R[264]=8) THEN ;
  91:  CALL PB_MID3    ;
  92:  ENDIF ;
  93:   ;
  94:  ! if one robot welds both sides ;
  95:  IF (R[290]<>0),JMP LBL[115] ;
  96:   ;
  97:  ! If not start of profile, jump t ;
 122:  IF ((R[95]=1) and (R[81]=2)) OR R[70]=55),JMP LBL[115] ;
  99:   ;
 100:  IF ((R[264]=4) OR (R[264]=8)),JMP LBL[105] ;
 101:  IF R[264]>=3,JMP LBL[999] ;
 102:  !**************************** ;
 103:  ! ***** WELDING ***** ;
 104:  ! ***** Sub points ***** ;
 105:  !**************************** ;
 106:   ;
 107:  !---------------------------- ;
 108:  !-- WELDING MOTION P5 ;
 109:  LBL[105] ;
 110:  R[153]=105    ;
 111:   ;
 112:  !using current angles ;
 113:  PR[120]=LPOS    ;
 114:  PR[5,4]=PR[120,4]    ;
 115:  PR[5,5]=PR[120,5]    ;
 116:  PR[5,6]=PR[120,6]    ;
